{
  "name": "prisma-openapi",
  "author": "Nitzan Ohana <16689354+nitzano@users.noreply.github.com>",
  "repository": "git@github.com:nitzano/prisma-openapi.git",
  "version": "1.4.2",
  "description": "",
  "main": "dist/index.js",
  "bin": {
    "prisma-openapi": "./dist/index.js"
  },
  "type": "module",
  "exports": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "clean": "rimraf dist prisma/openapi",
    "build": "tsc",
    "dev:gen": "pnpm clean && pnpm build && pnpm generate",
    "generate": "prisma generate",
    "dev": "tsc --watch",
    "lint": "xo",
    "lint:fix": "xo --fix",
    "format": "xo --fix",
    "prepare": "husky",
    "dev:release": "changeset",
    "psst": "psst-ai -f ./.github/copilot-instructions.md",
    "ver": "changeset",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "lint-staged": {
    "*.ts": "xo --fix"
  },
  "files": [
    "dist",
    "CHANGELOG.md"
  ],
  "keywords": [
    "prisma",
    "openapi",
    "swagger",
    "generator"
  ],
  "license": "MIT",
  "devDependencies": {
    "@changesets/cli": "^2.29.8",
    "@prisma/client": "^7.3.0",
    "@tsconfig/node22": "^22.0.5",
    "@types/node": "^25.2.0",
    "@typescript-eslint/eslint-plugin": "^8.54.0",
    "@typescript-eslint/parser": "^8.54.0",
    "@vitest/coverage-v8": "^4.0.18",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "prisma": "^7.3.0",
    "psst-ai": "^1.4.0",
    "rimraf": "^6.1.2",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18",
    "xo": "^1.2.3",
    "yaml": "^2.8.2"
  },
  "packageManager": "pnpm@10.28.2+sha512.41872f037ad22f7348e3b1debbaf7e867cfd448f2726d9cf74c08f19507c31d2c8e7a11525b983febc2df640b5438dee6023ebb1f84ed43cc2d654d2bc326264",
  "peerDependencies": {
    "prisma": "^7.0.0"
  },
  "dependencies": {
    "@prisma/generator-helper": "7.3.0",
    "@prisma/internals": "^7.3.0",
    "openapi3-ts": "^4.5.0",
    "winston": "^3.19.0"
  }
}
